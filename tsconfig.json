{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "ESNext"
    ],
    "moduleDetection": "force",
    "module": "preserve",
    "paths": {
    },
    "types": [
      "bun"
    ],
    "allowImportingTsExtensions": true,
    "allowJs": true,
    "strict": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noEmit": true,
    "noUncheckedSideEffectImports": true,
    "verbatimModuleSyntax": true,
    "skipLibCheck": true
  }
}
